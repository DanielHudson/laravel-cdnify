{
    "name": "metrique/laravel-cdnify",
    "description": "Add a CDN to any path when a specified environment is active.",
    "type": "Library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Hudson",
            "email": "support@shinobicorp.com",
            "homepage": "https://shinobicorp.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Metrique\\CDNify\\": "src/"
        }
    },
    "require": {
        "laravel/framework": "^5.5"
    },
    "require-dev": {
        "php": ">=5.5.9 || ^7.0",
        "league/flysystem-aws-s3-v3": "~1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Metrique\\CDNify\\CDNifyServiceProvider"
            ],
            "aliases": {
                "CDNify": "Metrique\\CDNify\\CDNifyFacade"
            }
        }
    }
}

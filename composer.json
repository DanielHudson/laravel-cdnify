{
    "name": "metrique/laravel-cdnify",
    "description": "Add a CDN to any path when a specified environment is active.",
    "type": "Library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Hudson",
            "email": "support@metrique.co.uk",
            "homepage": "https://metrique.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Metrique\\CDNify\\": "src/"
        }
    },
    "require": {
        "laravel/framework": ">=5.5"
    },
    "require-dev": {
        "php": "^7.0",
    },
    "extra": {
        "laravel": {
            "providers": [
                "Metrique\\CDNify\\CDNifyServiceProvider"
            ],
            "aliases": {
                "CDNify": "Metrique\\CDNify\\CDNifyFacade"
            }
        }
    }
}
